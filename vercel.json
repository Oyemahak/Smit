{
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": ".*"
        }
      ],
      "missing": [
        { "type": "file" },
        { "type": "directory" }
      ],
      "destination": "/"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}